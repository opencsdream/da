{
	"name": "test",
	"version": "0.1.0",
	"private": true,
	"scripts": {
		"serve": "vue-cli-service serve",
		"build": "vue-cli-service build",
		"lint": "vue-cli-service lint",
		"dist": "npm run build && npm run clean && npm run move",
		"move": "copyfiles -u 1 dist/css/** dist/img/** dist/js/** dist/favicon.ico dist/index.html ./",
		"clean": "rimraf css img js index.html favicon.png"
	},
	"dependencies": {
		"core-js": "^3.6.5",
		"vue": "^2.6.11"
	},
	"devDependencies": {
		"@vue/cli-plugin-babel": "~4.5.0",
		"@vue/cli-plugin-eslint": "~4.5.0",
		"@vue/cli-service": "~4.5.0",
		"babel-eslint": "^10.1.0",
		"copyfiles": "^2.4.1",
		"eslint": "^6.7.2",
		"eslint-plugin-vue": "^6.2.2",
		"rimraf": "^3.0.2",
		"vue-template-compiler": "^2.6.11"
	},
	"eslintConfig": {
		"root": true,
		"env": {
			"node": true
		},
		"extends": [
			"plugin:vue/essential",
			"eslint:recommended"
		],
		"parserOptions": {
			"parser": "babel-eslint"
		},
		"rules": {}
	},
	"browserslist": [
		"> 1%",
		"last 2 versions",
		"not dead"
	]
}
